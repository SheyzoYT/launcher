    "private": true,
    "main": "index.js",
    "scripts": {
        "start": "electron .",
        "dist": "electron-builder",
        "dist:win": "npm run dist -- -w"
    },
    "build": {
        "appId": "fr.Sheyzo.eronix",
        "productName": "Eronix Launcher",
        "win": {
            "icon": "./public/icons/512x512.png"
        },
        "files": [
            
